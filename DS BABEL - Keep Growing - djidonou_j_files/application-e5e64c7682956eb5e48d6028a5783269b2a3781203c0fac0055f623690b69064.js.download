function toggleFullScreen() {
    var a = $(window).height() - 10;

    if (!document.fullscreenElement && // alternative standard method
        !document.mozFullScreenElement && !document.webkitFullscreenElement) { // current working methods
        if (document.documentElement.requestFullscreen) {
            document.documentElement.requestFullscreen();
        } else if (document.documentElement.mozRequestFullScreen) {
            document.documentElement.mozRequestFullScreen();
        } else if (document.documentElement.webkitRequestFullscreen) {
            document.documentElement.webkitRequestFullscreen(Element.ALLOW_KEYBOARD_INPUT);
        }
    } else {
        if (document.cancelFullScreen) {
            document.cancelFullScreen();
        } else if (document.mozCancelFullScreen) {
            document.mozCancelFullScreen();
        } else if (document.webkitCancelFullScreen) {
            document.webkitCancelFullScreen();
        }
    }
    $('.full-screen').toggleClass('icon-maximize');
    $('.full-screen').toggleClass('icon-minimize');
};
CanvasRenderingContext2D.prototype.roundRect = function (x, y, w, h, r) {
  if (w < 2 * r)
    r = w / 2;
  if (h < 2 * r)
    r = h / 2;
  this.beginPath();
  this.moveTo(x+r, y);
  this.arcTo(x+w, y,   x+w, y+h, r);
  this.arcTo(x+w, y+h, x,   y+h, r);
  this.arcTo(x,   y+h, x,   y,   r);
  this.arcTo(x,   y,   x+w, y,   r);
  this.closePath();
  return this;
}
class Box {
  constructor(name, x, y, width, height, link, margin, box_color, box_radius, text_font_size, text_font, text_color) {
    this.name = name;
    this.x = x;
    this.y = y;
    this.width = width;
    this.height = height;
    this.link   = link;
    this.margin = Math.min(5, margin);
    this.box_color = box_color
    this.box_radius = box_radius;
    this.text_font_size = text_font_size;
    this.text_font = text_font;
    this.text_color = text_color;
    this.high_lighted = false;
  }
  
  intersect(_x, _y) {
    return (this.x + this.margin) <= _x && _x <= (this.x + (this.width - (this.margin))) && (this.y + this.margin) <= _y && _y <= (this.y + (this.height- this.margin));
  }

  render(canvas) {
    let box_color, text_color;
    
    if (this.high_lighted == false) {
      box_color = this.box_color;
      text_color = this.text_color;
    }
    else {
      box_color = this.text_color;
      text_color = this.box_color;
