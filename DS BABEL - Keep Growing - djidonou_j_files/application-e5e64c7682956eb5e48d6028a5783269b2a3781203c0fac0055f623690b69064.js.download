        return "#4fb9e9";
      }
      else if (status == 'approved') {
        return "#73bc73";
      }
      else if (status == 'rejected') {
        return "#b85354";
      }
      else {
        return "#b0dbf3";
      }
    }

    calculate_position() {
      let index = 0;
      let kndex = 0;

      let previous_point = new Point(0, this.beginning_line_y + (this.box_height/2));
      let end_canvas_point = new Point(this.canvas_width, this.beginning_line_y + (this.box_height/2));
      while (index < this.map.length) {
        let jndex = 0;

        while (jndex < this.map[index].length) {
          let x = this.canvas_margin+(this.map[index][jndex]['begin_at'] / this.ratio);
          let y = this.beginning_line_y + (this.box_height * index);
          let box_width = ((this.map[index][jndex]['end_at'] - this.map[index][jndex]['begin_at']) / this.ratio) - (this.margin * 2);

          var box_color = this.box_colors(this.map[index][jndex]['status']);
          let box = new Box(this.map[index][jndex]['name'], x, y, box_width, this.box_height,  this.map[index][jndex]['link'], this.margin, box_color, this.box_radius, this.text_font_size, this.text_font, this.text_color);
          this.boxes.push(box);

          // Line through boxes
          let point_begin = new Point(box.x, box.y+(this.box_height/2));
          let point_end   = new Point(box.x+box.width, box.y+(this.box_height/2));
          this.lines.push(new Line(point_begin, point_end, this.line_size, this.line_color));

          // Openning boxes

          if (previous_point.y != point_begin.y) {
            kndex = this.find_previous_intersection(index, point_begin.x);
            point_begin.y += this.line_size/2;
            
            previous_point = new Point(point_begin.x, point_begin.y - (this.box_height * (index-kndex)));
          }
          this.lines.push(new Line(previous_point, point_begin, this.line_size, this.line_color));

          previous_point = point_end;
          jndex += 1;

          // Close boxes
          if (index > 0 && (jndex == this.map[index].length || (previous_point.x + (this.margin * 2) < (this.canvas_margin+(this.map[index][jndex]['begin_at'] / this.ratio) - this.margin)))) {
            kndex = this.find_previous_intersection(index, point_begin.x);
            previous_point.y += this.line_size/2;
            let tmp_previous_point = new Point(previous_point.x, previous_point.y - (this.box_height * (index-kndex)));
            this.lines.push(new Line(previous_point, tmp_previous_point, this.line_size, this.line_color));
            previous_point = tmp_previous_point;
          }
          else if (index == 0 && jndex == this.map[index].length) {
            this.lines.push(new Line(previous_point, end_canvas_point, this.line_size, this.line_color));
          }
        }
        index += 1;
      }
    }
    
    on_click(e) {
