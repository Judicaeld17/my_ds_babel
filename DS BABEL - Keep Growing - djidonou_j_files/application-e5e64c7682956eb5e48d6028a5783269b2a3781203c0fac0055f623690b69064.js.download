      $(this.canvas).parent().append("<i id='project-road-tooltip' data-toggle='tooltip' data-placement='top' title='Tooltip for image' data-animation='false' data-trigger='manual' style='position: absolute;' />");

      this.ctx = this.canvas.getContext("2d");

      this.canvas.width = ($(this.canvas).parent().width() * antialiazing);
      this.canvas.height = ($(this.canvas).parent().height() * antialiazing);
      this.canvas_width = (this.canvas.width / antialiazing);
