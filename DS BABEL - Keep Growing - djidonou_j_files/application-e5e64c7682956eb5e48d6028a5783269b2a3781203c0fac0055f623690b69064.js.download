  $(".form-radio").each(function(_, elem) {
    $(elem).find('.radio-inline input').each(function(index, sub_elem) {
      if (sub_elem.name.match(/peer_review\[answers\]/g)) {
        if (sub_elem.value == 'No') {
          sub_elem.checked = true;
        }
        else if (sub_elem.value != 'Yes' && index == 0) {
          sub_elem.checked = true;
        }
