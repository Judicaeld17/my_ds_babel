        x = ev.layerX;
        y = ev.layerY;
      }
      x -= this.canvas.offsetLeft;
      y -= this.canvas.offsetTop;

      while (index < this.boxes.length) {
        let box = this.boxes[index];
//        console.log("X -- %d <= %d <= %d", box.x, x, (box.x + box.width));
//        console.log("Y -- %d <= %d <= %d", box.y, y, (box.y + box.height));
        if (box.intersect(x, y)){
