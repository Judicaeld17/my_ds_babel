function repeat_reload_control_center() {
  route = 'solutions'
  solution_id = $("#metadata_project").data('solution-id');
  if (solution_id == undefined) {
    route = 'project_solutions'
    solution_id = $("#metadata_project").data('project-solution-id');
  }
  user_id = $("#metadata_project").data('user-id');
  if (solution_id && user_id) {
    setTimeout(function () {
      var url = window.location.protocol + "//" + window.location.host + "/"+route+"/" + solution_id + "/control_center?user_id="+user_id;

      console.log(url);
      $.ajax({
          type: "GET",
          contentType: "application/json",
          url: url,
          success: function(data){
            reload_control_center_timer = Math.min(reload_control_center_timer * 2, 120000); // 2 minutes
            repeat_reload_control_center();
          }
        });
    }, window.reload_control_center_timer);
  }
}

function reload_control_center() {
  if ($('#project_control_center').length) {
    window.reload_control_center_timer = 5000;
    repeat_reload_control_center();
  }
}

function repeat_reload_roleplay(elem, roleplay_id) {
  not_reloadable_statuses = ['debrief-false', 'finished'];

  setTimeout(function () {
    $.ajax({
        type: "GET",
        contentType: "application/json",      
        url: window.location.origin + "/roleplay_solutions/" + roleplay_id,
        success: function(data) {
          var status = $(elem).children(":first").data('roleplay-status');
          if (not_reloadable_statuses.includes(status) == false) {
            repeat_reload_roleplay(elem, roleplay_id);
          }
        }
      });
  }, 5000);
}

function reload_roleplay() {
  not_reloadable_statuses = ['debrief-false', 'finished'];

  $(".roleplay_iteration").each(function(_, elem) {
    var status = $(elem).children(":first").data('roleplay-status');
    if (not_reloadable_statuses.includes(status) == false) {
      roleplay_id = elem.id.replace('roleplay_solution_', '')
      repeat_reload_roleplay(elem, roleplay_id);
    }
  });
}

