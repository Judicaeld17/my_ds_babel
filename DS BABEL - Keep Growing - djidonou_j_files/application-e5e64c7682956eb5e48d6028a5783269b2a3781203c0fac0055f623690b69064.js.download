'use strict';
$(document).ready(function() {
    floatchart()
    $(window).on('resize', function() {
        floatchart();
    });
    $('#mobile-collapse').on('click', function() {
        setTimeout(function() {
            floatchart();
        }, 700);
    });
    $(".scroll-widget").slimScroll({
        size: "5px",
        height: "500px",
        allowPageScroll: false,
    });

    $(".scroll-widget-50").slimScroll({
        size: "5px",
        height: "50vh",
        allowPageScroll: false,
    });

    $(".scroll-widget-30").slimScroll({
        size: "5px",
        height: "30vh",
        allowPageScroll: false,
    });
    $(".scroll-widget-30-bottom").slimScroll({
        size: "5px",
        height: "30vh",
        start: 'bottom',
        allowPageScroll: false,
    });
    $(".scroll-widget-67").slimScroll({
        size: "5px",
        height: "67vh",
        allowPageScroll: false,
    });
    $(".scroll-widget-67-bottom").slimScroll({
        size: "5px",
        height: "67vh",
        start: 'bottom',
        allowPageScroll: false,
    });
    $(".scroll-widget-full-size").slimScroll({
        size: "5px",
        height: "calc(100vh - 170px)",
        allowPageScroll: false,
    });
    $(".scroll-widget-agora").slimScroll({
        size: "5px",
        height: "calc(100vh - 170px)",
        start: 'bottom',
        allowPageScroll: false,
    });
});

function floatchart() {
    $(function() {
