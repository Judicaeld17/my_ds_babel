function modify_qpoint() {
  var elements = document.querySelectorAll('.button_to,.qpoints');

  for (var i = 0; i < elements.length; i++) {
    elements[i].addEventListener('ajax:success', function(event) {
      var detail = event.detail;
      var status = detail[1];
      if (status == "OK") {
        var new_qpoints_value = detail[0];
        document.getElementById("user_qpoints").textContent = new_qpoints_value;
      }
    })
  }

}

function peer_review_activate_dot(khis) {
  $(khis).parents('.row.p-b-30').find('.update-icon').addClass('completed');
}

function polar_chart() {
  var polarElem = document.getElementById("polarChart");

  if (polarElem == undefined)
    return ;
    nbr_problem_solving = $(polarElem).data('softskills').problemsolving;
    nbr_creativity = $(polarElem).data('softskills').creativity;
    nbr_empathy = $(polarElem).data('softskills').empathy;
    nbr_collaboration = $(polarElem).data('softskills').collaboration;
    nbr_communication = $(polarElem).data('softskills').communication;
    colors = $(polarElem).data('colors');
    var data3 = {
        datasets: [{
            data: [
                nbr_problem_solving,
                nbr_creativity,
                nbr_empathy,
                nbr_collaboration,
                nbr_communication
            ],
            borderWidth: 0,
            backgroundColor: [
                colors[0],
                colors[1],
                colors[2],
                colors[3],
                colors[4]
            ],
            hoverBackgroundColor: [],
            label: 'Soft Skills' // for legend
        }],
        labels: [
            "Problem Solving",
            "Creativity",
            "Empathy",
            "Collaboration",
            "Communication"
        ]
    };

    new Chart(polarElem, {
        data: data3,
        type: 'polarArea',
        options: {
          scale: {
              display: false
            },
            legend: {
              position: false
            },
            elements: {
              arc: {
                borderColor: "#fff"
              }
            }
        }
    });
}

function pie_char() {
  var pieElem = document.getElementById("pieChart");
  if (pieElem == undefined)
    return ;

  var data4 = {
      labels: [
          "In progress",
          "Finished"
      ],
      datasets: [{
          data: [20, 80],
          backgroundColor: [
              "#2a879e",
              "#4fb9e9"
          ]
      }]
  };

  var myPieChart = new Chart(pieElem, {
      type: 'pie',
      data: data4,
      options: {
        legend: {
          position: 'bottom'
        }
      }
  });
}

function bar_char() {
  var barElem = document.getElementById("barChart");
  if (barElem == undefined)
    return ;

    data = $(barElem).data('data');

    name = $(barElem).data('name');

    color = $(barElem).data('color');

    colors = []
    keys   = []
    values = []
    for (index = 0; index < data.length; index++) {
      keys.push(data[index].key);
      values.push(data[index].value);
      colors.push(color);
    }

    var data1 = {
        labels: keys,
        datasets: [{
            label: name,
            backgroundColor: colors,
            data: values,
        }]
    };

    var myBarChart = new Chart(barElem, {
        type: 'bar',
        data: data1,
        options: {
            barValueSpacing: 20
        }
    });
}

function line_char() {
  $(".lineChart").each(function(_, line_chart) {
      data = $(line_chart).data('data');

      colors = $(line_chart).data('colors');
      y_unity = '';

      if ($(line_chart).data('yunity') != undefined) {
        y_unity = $(line_chart).data('yunity');
      }

      lines = []
      keys = []
      for (jndex in data) {
        key = data[jndex][0]
        value = data[jndex][1]
        index = 0;
        keys.push(key);
        for (subelem in value) {
          if (lines[index] == undefined) {
            lines[index] = {
              label: subelem.capitalize(),
              backgroundColor: colors[index],
              borderColor: colors[index],
              data: [],
              fill: false
            }
          }
          lines[index].data.push(value[subelem]);
          index += 1;
        }
      }
      var config = {
          type: 'line',
          data: {
            labels: keys,
            datasets: lines
          },
          options: {
            scales: {
              yAxes: [{
                scaleLabel: {
                  display: true,
                  labelString: y_unity
                }
              }]
            },
            tooltips: {
              callbacks: {
                label: function(tooltipItem, data) {
                  return tooltipItem.yLabel.toFixed(1).toString();
                }
              }
            }
          }
        };

    var chartInstance1 = new Chart(line_chart, config);
  });
}


function to_percent(nbr, total) {
  return ((nbr/total) * 100);
}

function progress_line() {
  $(".progress-multiple").each(function(_, elem) {
    if ($(elem).data('progress')) {
      keys = Object.keys($(elem).data('progress'));
      values = Object.values($(elem).data('progress'));
      total = (values.reduce((a, b) => a + parseInt(b), 0))
      nbr_1 = to_percent(parseInt(values[0]), total);
      nbr_2 = to_percent(parseInt(values[1]), total);
      nbr_3 = to_percent(parseInt(values[2]), total);
      nbr_4 = to_percent(parseInt(values[3]), total);
      nbr_5 = to_percent(parseInt(values[4]), total);
      $(elem).css('background-size', nbr_1.toString()+'% 100%, '+ (nbr_2+nbr_1).toString() +'% 100%, '+ (nbr_3+nbr_2+nbr_1).toString() +'% 100%, '+ (nbr_4+nbr_3+nbr_2+nbr_1).toString() +'% 100%, '+ (nbr_5+nbr_4+nbr_3+nbr_2+nbr_1).toString() +'% 100%');
    }
  });
}

