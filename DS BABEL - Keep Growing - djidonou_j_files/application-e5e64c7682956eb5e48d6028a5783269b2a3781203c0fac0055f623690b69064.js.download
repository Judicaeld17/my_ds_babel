            box.high_lighted = true;
            this.draw();
          }
          this.current_box = box;
          return ;
        }
        index += 1;
      }
      $('#project-road-tooltip').tooltip('hide')
      document.body.style.cursor = "";
      if (this.current_box) {
        this.unhighlighted_all_boxes();
        this.draw();
      }
      this.current_box = null;
    }
    
    draw_lines() {
      let index = 0;
      while (index < this.lines.length) {
        let line = this.lines[index];

//        line.print();
        line.render(this.ctx);
        index += 1;
      }
    }
    
    draw_boxes() {
      let index = 0;
      while (index < this.boxes.length) {
        let box = this.boxes[index];
        box.render(this.ctx);
        index += 1;
      }
    }
    
    render() {
      this.init();
      this.calculate_position();
      this.draw();
    }
    
    draw() {
      this.ctx.clearRect(0, 0, this.canvas.width, this.canvas.height);
      this.draw_lines();
      this.draw_boxes();
    }
    
    resize() {
      this.render();
    }
