function init_countdown() {
  $('[data-countdown]').each(function() {
    $(this).countdown($(this).data('countdown'), {elapse: true}).on('update.countdown', function(event) {
      var $this = $(this);
      if (event.elapsed) {
        if ($this.data('after-url')) {
          window.location = window.location.origin+$this.data('after-url');
        }
        else {
          $this.html(event.strftime('<span>%H:%M:%S</span> (extra time)'));
        }
      }
      else {
        $this.html(event.strftime('<span>%H:%M:%S</span>'));
      }
    });
  });
}

jQuery.fn.preventDoubleClick = function() {
  $(this).on('click', function(e){
    var $el = $(this);
    if($el.data('clicked')){
      // Previously clicked, stop actions
      e.preventDefault();
      e.stopPropagation();
    } else {
      $el.data('clicked', true);
      // Unmark after 1 second
      window.setTimeout(function(){
        $el.removeData('clicked');
      }, 3000)
    }
  });
  return this;
};

