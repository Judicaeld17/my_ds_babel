      this.current_box = null;
      
      let values = this.get_min_max(this.map);
      let min = values[0];
      let max = values[1];
      let nbr_elems = values[2];

      this.margin = Math.min(this.canvas_width * (0.25 / nbr_elems), this.canvas_height * (0.25 / nbr_elems));
      this.canvas_margin = this.canvas_width * 0.01;
      this.line_size = this.canvas_height * 0.03;
      this.boxes = [];
      this.lines = [];

