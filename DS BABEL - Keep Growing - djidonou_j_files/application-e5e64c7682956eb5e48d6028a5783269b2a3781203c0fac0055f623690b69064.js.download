    unhighlighted_all_boxes() {
      let index = 0;
      
      while (index < this.boxes.length) {
        this.boxes[index].high_lighted = false;
        index += 1;
      }
    }

    get_min_max(map) {
      let min = 0;
      let max = 0;
      let index = 0;
      let count_elems = 0;

      while (index < map.length) {
        let jndex = 0;
        while (jndex < map[index].length) {
          if (map[index][jndex]['end_at'] > max) {
            max = map[index][jndex]['end_at'];
          }
          if (map[index][jndex]['begin_at'] < min) {
            min = map[index][jndex]['begin_at'];
          }
          jndex += 1;
        }
        if (map[index].length > count_elems) {
          count_elems = map[index].length;
        }
        index += 1;
      }
      
      return [min, max, count_elems];
    }
    
    is_there_box_in_line(row, x) {
      let index = 0;
      while (index < row.length) {
        let begin_x = this.canvas_margin+(row[index]['begin_at'] / this.ratio) - this.margin;
        let end_x = begin_x+((row[index]['end_at'] - row[index]['begin_at']) / this.ratio) - (this.margin * 2);
        if (begin_x < x && x < end_x) {
          return true
        }
        index += 1;
      }
      return false;
    }
    
    find_previous_intersection(current_index, current_x) {
      let kndex;
      kndex = Math.max(current_index - 1, 0);
      while (kndex > 0 && this.is_there_box_in_line(this.map[kndex], current_x) == false) {
        kndex -= 1;
      }
      return kndex;
